{
  "cache": {
    "name": "Disk",
    "path": "/tmp/stache",
    "umask": "0000",
    "dirs": "portable"
  },
  "layers": {
    "simple": {
      "provider": {
        "name": "mapnik",
        "mapfile": "mapstyle-simple.xml"
      },
      "projection": "spherical mercator",
      "preview": {
        "lat": 54.5302,
        "lon": 36.2597,
        "zoom": 12
      },
      "metatile": {
        "rows": 4,
        "columns": 4,
        "buffer": 0
      },
      "png options": {
        "optimize": true
      }
    },
    "grid": {
      "provider": {
        "name": "mapnik grid",
        "mapfile": "mapstyle-simple.xml",
        "scale": 4,
        "layers": [
          [0, ["name"]],
          [7, ["name"]],
          [9, ["housenumber", "street"]]
        ]
      },
      "projection": "spherical mercator",
      "preview": {
        "lat": 54.5302,
        "lon": 36.2597,
        "zoom": 12
      }
    }
  },
  "index": "index.html"
}
